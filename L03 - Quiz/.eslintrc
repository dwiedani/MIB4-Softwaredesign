{
	"env": {
		"es6": true,
	},
	"extends": ["airbnb-typescript"],
	"rules": {
		"no-console": 0,
		"no-unused-vars": 0,
		'no-plusplus': 'off',
		'no-tabs': 0,
		'no-mixed-spaces-and-tabs': 0,
		'import/prefer-default-export': 'off',
		'@typescript-eslint/no-unused-vars': 0,
	},
	parserOptions: {
    	project: 'tsconfig.json',
  	},
}
